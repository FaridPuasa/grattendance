<!-- attendanceLog.ejs -->
<!DOCTYPE html>
<html>
<head>
    <title>Attendance Log</title>
</head>
<body>
    <h1>Attendance Log</h1>
    <table border="1">
        <tr>
            <th>Username</th>
            <th>Location</th>
            <th>Sign In time</th>
            <th>Sign Out time</th>
            <th>Date</th>
        </tr>
        <% attendanceData.forEach(entry => { %>
            <tr>
                <td><%= entry.username %></td>
                <td><%= entry.location %></td>
                <td><%= entry.signIn %></td>
                <td><%= entry.signOut %></td>
                <td><%= entry.date %></td>
            </tr>
        <% }); %>
    </table>
</body>
</html>
